local long 		lTemp;
local boolean 	result;

{Hook to clear out the current messenger address fields and remove them from reference list}
PresenceChangeEventTrigger = -1;
run script PresenceChangeEventTrigger;
run script Label_Presence[1];

{--------------------------------------------------------------------------------
	Check for a note, and display the proper note button.
--------------------------------------------------------------------------------}
run script 'Dummy Note Show Hide';

{--------------------------------------------------------------------------------
	Since there is no record being displayed yet there can't be a record note.
--------------------------------------------------------------------------------}
hide 'Note Present Button- Window Area';
show 'Note Absent Button - Window Area';

{--------------------------------------------------------------------------------
	Initialize the window.
--------------------------------------------------------------------------------}
clear coWriteLettersBDL;
add item getmsg(5413), 1 to coWriteLettersBDL;		{Preoare an Employee Letter}
{--- Don't show if Small Business Manager ---}
if 'SQL Server' of globals <> DYNAMICSLITE then
	if ('Module Registered'[UPR] of globals and 'Module Loaded'[UPR] of globals)
		or RegisteredHR(SUPPRESS_MESSAGE) then
		add item getmsg(7998), 2 to coWriteLettersBDL;  	{Prepare an Employee Benefits and Deductions Letter}
	end if;
	{--- Only show if HR is loaded and registerd ---}
	if RegisteredHR(SUPPRESS_MESSAGE) then
		add item getmsg(7999), 3 to coWriteLettersBDL;		{Prepare an Employee Skills and Tests Letter}
		add item getmsg(8090), 4 to coWriteLettersBDL;		{Prepare an Employee Reviews Letter}
	end if;
end if;
add item getmsg(5401), 5 to coWriteLettersBDL;

run script Inactive;

'Work Hours Per Year' of window UPR_Employee_MNT_Additional = 2080;
Gender of window UPR_Employee_MNT_Additional = 3;  			{N/A}
'Ethnic Origin' of window UPR_Employee_MNT_Additional = 7;	{N/A}
'MaritalStatus' of window UPR_Employee_MNT_Additional = 3;	{N/A}
'HR Status' of window UPR_Employee_MNT_HR = 1; 			  	{Active}
'Type of Employment' = 1; 									{Full Time Regular}
run script 'Which Cash Account For Pay' of window UPR_Employee_MNT_Additional;

if isopen(window 'UPR_Employee_MNT_Vacation/Sick') then
	run script 'Auto Accrue Vacation' of window 'UPR_Employee_MNT_Vacation/Sick';
	run script 'Auto Accrue Sick Time' of window 'UPR_Employee_MNT_Vacation/Sick';
end if;

if isopen(window UPR_Employee_MNT_Address) then
	clear window UPR_Employee_MNT_Address;
end if;

if not 'Module Registered'[UPR] of globals then
{Changes for HR Stand Alone for Employee Maintenance window}
		{04Sep01 Start}
		if not 'Module Registered'[PA] of globals then 		
			disable 'Employee Class' of window UPR_Employee_MNT;
			disable 'Zoom Buttons'[1];
		end if;
		{04Sep01 End}
		disable 'Suta State' of window UPR_Employee_MNT;
