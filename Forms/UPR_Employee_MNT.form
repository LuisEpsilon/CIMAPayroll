		EventScript	"UPR_Employee_MNT EX_PR_Skip_CHG"		
		{
			Documentation	""
			Script	"{--------------------------------------------------------
		Cannot inactivate if activity exists for employee.
	--------------------------------------------------------}
if EX_PR_Skip of window UPR_Employee_MNT of form UPR_Employee_MNT then

	set 'Employee ID' of file UPR_WORK_HDR to 'Employee ID';
	get file UPR_WORK_HDR by UPR_WORK_HDR_Key3;
	if err() = OKAY then
	
		{debug technicalname(table UPR_WORK_HDR);}

		call Warning_Dialog, 10265, \"\",\"\",\"\";
		{For: Sorry, the employee record cannot be inactivated or deleted.
				Pay run activity in progress for this employee.}
									
		{set Inactive to false;}
		
		EX_PR_Skip = false;
		abort script;
	else
		EX_PR_Skip = true;
		EX_PR_Skip_Set_By = 'User ID' of globals;
		EX_PR_Skip_Set_Date = sysdate();
	end if;
	
	{--------------------------------------------------------------------
		Cannot delete if there are any records in the batch detail file.
	--------------------------------------------------------------------}
	clear file UPR_TRX_Batch_Detail;
	set 'Employee ID' of file UPR_TRX_Batch_Detail to 'Employee ID';
	get next file UPR_TRX_Batch_Detail by UPR_TRX_Batch_Detail_Key4;
	if err() = OKAY and 'Employee ID' of file UPR_TRX_Batch_Detail = 'Employee ID' then

		{debug technicalname(table UPR_TRX_Batch_Detail);}
		
		call Warning_Dialog,10010,\"\",\"\",\"\";
		{For:  Sorry, there are unposted transactions for this employee.
				The employee record cannot be deleted or inactivated.}

		{set Inactive to false;}
		
		EX_PR_Skip = false;
		abort script;
	else
		EX_PR_Skip = true;
		EX_PR_Skip_Set_By = 'User ID' of globals;
		EX_PR_Skip_Set_Date = sysdate();
	end if;
else	
	EX_PR_Skip_UnSet_By = 'User ID' of globals;
	EX_PR_Skip_UnSet_Date = sysdate();
end if;	"
			UserProperties	""
		}
