		EventScript	"EX_Employee_Cayman_WIN_PRE"		
		{
			Documentation	""
			Script	"local long l_sql_connect;
local long l_sql_status;
local long l_sql_data_status;
local text l_sql_command,l_text;

local string l_permit_type;

{Populate the permit type combo-box}

	l_sql_status = SQL_Connect(l_sql_connect);
	l_sql_command = \"USE \" + 'Intercompany ID' of globals;
	l_sql_status  = SQL_Execute(l_sql_connect,l_sql_command);
	l_sql_status = SQL_Clear(l_sql_connect);	

	l_sql_command = \"select distinct(EX_Permit_Type) from EX00100 where EX_Permit_Type <> ''\";
	l_sql_status  = SQL_Execute(l_sql_connect,l_sql_command);
	
	l_sql_status = SQL_FetchNext(l_sql_connect);

	while l_sql_status <> 31 do
		l_sql_data_status = SQL_GetData(l_sql_connect,1,l_permit_type);
		if finditem(EX_Permit_Type of window EX_Employee_Cayman of form EX_Employee_Cayman, l_permit_type) = 0 then	
			add item l_permit_type to field EX_Permit_Type of window EX_Employee_Cayman of form EX_Employee_Cayman;
		end if;
		l_sql_status = SQL_FetchNext(l_sql_connect);
	end while;
	l_sql_status  = SQL_Clear(l_sql_connect);
	l_sql_status  = SQL_Terminate(l_sql_connect);
	
	

release table EX_Employee_MSTR;
clear table EX_Employee_MSTR;
set 'Employee ID' of table EX_Employee_MSTR to 'Employee ID' of window UPR_Employee_MNT of form UPR_Employee_MNT;
get table EX_Employee_MSTR;

if err() = 0 then
	copy from table EX_Employee_MSTR to  window EX_Employee_Cayman of form EX_Employee_Cayman;
else
	change table EX_Employee_MSTR;
	copy from table EX_Employee_MSTR to  window EX_Employee_Cayman of form EX_Employee_Cayman;
	save table EX_Employee_MSTR;
end if;"
			UserProperties	""
		}
		EventScript	"Window1 EX_Permit_Type_CHG"		
		{
			Documentation	""
			Script	"local integer l_ask;

if finditem(EX_Permit_Type, EX_Permit_Type) = 0 then	
	l_ask = ask(\"Are you sure you want to add this permit type?\", \"Yes\", \"No\",\"\");

	if l_ask = 1 then
		add item EX_Permit_Type to field EX_Permit_Type of window EX_Employee_Cayman of form EX_Employee_Cayman;
		redraw field EX_Permit_Type;
	else
		clear field EX_Permit_Type;
	end if;
end if;"
			UserProperties	""
		}
