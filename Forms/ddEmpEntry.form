	~EventScripts
	{
		EventScript	"Scroll Bank ID_CHG"		
		{
			Documentation	""
			Script	"
if empty ('Bank ID') then
	clear 'DD Transit Number' of window Scroll of form ddEmpEntry;
	clear 'DD Account Number' of window Scroll of form ddEmpEntry;
else

	release table CM_Bank_MSTR;
	clear table CM_Bank_MSTR;
	set 'Bank ID' of table CM_Bank_MSTR to 'Bank ID';
	get table CM_Bank_MSTR;

	if err() = 0 then
		'DD Transit Number' = 'DD Transit Number' of table CM_Bank_MSTR;
		focus field 'DD Account Number';
		force changes window ddEmpEntry;
		
		release table EX_DD_Banks_Temp;
		clear table EX_DD_Banks_Temp;
		set 'Employee ID' of table EX_DD_Banks_Temp to
			'Employee ID' of window ddEmpEntry;
		set Index of table EX_DD_Banks_Temp to IndexLong;
		change table EX_DD_Banks_Temp;
		set 'Bank ID' of table EX_DD_Banks_Temp to 'Bank ID';
		save table EX_DD_Banks_Temp;

		{if empty ('(L) ref_EX_DD_Banks_Temp' of window ddEmpEntry of form ddEmpEntry) then
			release table ('(L) ref_EX_DD_Banks_Temp' of window ddEmpEntry of form ddEmpEntry);
			get first table ('(L) ref_EX_DD_Banks_Temp' of window ddEmpEntry of form ddEmpEntry);
			{sTable = Table_GetOSName(table EX_DD_Banks_Temp);}

			clear table ('(L) ref_EX_DD_Banks_Temp' of window ddEmpEntry of form ddEmpEntry);
			set 'Employee ID' of table ('(L) ref_EX_DD_Banks_Temp' of window ddEmpEntry of form ddEmpEntry) to 'Employee ID' of window ddEmpEntry of form ddEmpEntry;
			set Index of table ('(L) ref_EX_DD_Banks_Temp' of window ddEmpEntry of form ddEmpEntry) to IndexLong of window Scroll of form ddEmpEntry;
			get table ('(L) ref_EX_DD_Banks_Temp' of window ddEmpEntry of form ddEmpEntry);
			if err() = OKAY then
				set 'Bank ID' of window Scroll of form ddEmpEntry to 'Bank ID' of table ('(L) ref_EX_DD_Banks_Temp' of window ddEmpEntry of form ddEmpEntry);
					
			else
				clear 'Bank ID' of window Scroll of form ddEmpEntry;
			end if;
		end if;}
	else
		warning \"This Bank doesnt exist!\";
		focus field 'Bank ID';
	end if;
end if;"
			UserProperties	""
		}
		EventScript	"Scroll Lookup Button 13_CHG"		
		{
			Documentation	""
			Script	"run script 'Lookup Button 2' of window ddEmpEntry;"
			UserProperties	""
		}
		EventScript	"ddEmpEntry Lookup Button 2_CHG"		
		{
			Documentation	""
			Script	"if empty (IndexLong of window Scroll) then
	warning \"You need to select a line before opening the Bank lookup window\";
	restart field;
	abort script;
else
	if not isopen(window Bank_Lookup of form Bank_Lookup) then
		open form Bank_Lookup return to 'Bank ID' of window Scroll;
		if empty('Bank ID' of window Scroll) then
			set '(L) Bank Sort' of window Bank_Lookup of form Bank_Lookup to 1;
		end if;
	else
		open form Bank_Lookup return to 'Bank ID' of window Scroll;
	end if;

	set 'Bank ID' of window Bank_Lookup of form Bank_Lookup to 'Bank ID' of window Scroll;

	run script '(L) Bank Sort' of window Bank_Lookup of form Bank_Lookup;
end if;"
			UserProperties	""
		}
		EventScript	"ddEmpEntry WINPRE_CHG"		
		{
			Documentation	""
			Script	"range clear table RNC_ddEmpAcctMstr_TMP;
remove range table RNC_ddEmpAcctMstr_TMP;

release table RNC_ddEmpAcctMstr_TMP;
get first table RNC_ddEmpAcctMstr_TMP;

set 'Table Physical Name' of window ddEmpEntry to Table_GetOSName(table RNC_ddEmpAcctMstr_TMP);

assign '(L) ref_EX_DD_Banks_Temp' of window ddEmpEntry of form ddEmpEntry as reference to table EX_DD_Banks_Temp;
assign '(L) ref_RNC_ddEmpAcctMstr_TMP' of window ddEmpEntry of form ddEmpEntry as reference to table RNC_ddEmpAcctMstr_TMP;
"
			UserProperties	""
		}
