		EventScript	"ddEmpEntry l_SAE_ClearEmployeeID_CHG"		
		{
			Documentation	""
			Script	"
{range clear table ('(L) ref_EX_DD_Banks_Temp' of window ddEmpEntry of form ddEmpEntry);
remove range table ('(L) ref_EX_DD_Banks_Temp' of window ddEmpEntry of form ddEmpEntry);}

range clear table ('(L) ref_RNC_ddEmpAcctMstr_TMP' of window ddEmpEntry of form ddEmpEntry);
remove range table ('(L) ref_RNC_ddEmpAcctMstr_TMP' of window ddEmpEntry of form ddEmpEntry);"
			UserProperties	""
		}
		EventScript	"ddEmpEntry l_l_EX_DER_CHG"		
		{
			Documentation	""
			Script	"{local string l_where;

if 'Display Existing Record' of window ddEmpEntry of form ddEmpEntry  then
	close table EX_DD_Banks_Temp;
	
	l_where = \"EMPLOYID = '\" + 'Employee ID' of window ddEmpEntry of form ddEmpEntry + \"'\";
	
	release table EX_DD_Banks;
	range clear table EX_DD_Banks;
	range table EX_DD_Banks where l_where;
	
	range copy table EX_DD_Banks to table EX_DD_Banks_Temp;

end if;
	}"
			UserProperties	""
		}
		EventScript	"ddEmpEntry l_l_Scroll_Fill_CHG"		
		{
			Documentation	""
			Script	"{}
if isopen(window ddEmpEntry of form ddEmpEntry) then
	release table EX_DD_Banks;
	clear table EX_DD_Banks;
	set 'Employee ID' of table EX_DD_Banks to
		'Employee ID' of window ddEmpEntry of form ddEmpEntry;
	set Index of table EX_DD_Banks to
		IndexLong of table ddEmpLineTemp;
	get table EX_DD_Banks;
	set 'Bank ID' of window Scroll of form ddEmpEntry to
		'Bank ID' of table EX_DD_Banks;
		
		{warning('Bank ID' of window Scroll of form ddEmpEntry);}
end if;"
			UserProperties	""
		}
